{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    },
    {
      "src": ".vercel/output/functions/_qwik-city.func/**/*",
      "use": "@vercel/edge"
    }
  ],
  "routes": [
    {
      "src": "/(.*)?service-worker.js",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      },
      "continue": true
    },
    {
      "src": "/(assets|build)/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, s-maxage=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": ".vercel/output/functions/_qwik-city.func"
    }
  ]
}